<p id="notice"><%= notice %></p>

<h1>Volunteer Taggings</h1>

<table>
  <thead>
    <tr>
      <th>Volunteer</th>
      <th>Volunteer tag</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @volunteer_taggings.each do |volunteer_tagging| %>
      <tr>
        <td><%= volunteer_tagging.volunteer %></td>
        <td><%= volunteer_tagging.volunteer_tag %></td>
        <td><%= volunteer_tagging.organization %></td>
        <td><%= link_to 'Show', volunteer_tagging %></td>
        <td><%= link_to 'Edit', edit_volunteer_tagging_path(volunteer_tagging) %></td>
        <td><%= link_to 'Destroy', volunteer_tagging, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Volunteer Tagging', new_volunteer_tagging_path %>
